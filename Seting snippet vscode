	"This Load View CodeIgniter" : {
		"prefix": "thisv",
		"body": "\\$this->load->view('$1');$0"
	},
	"This Load Model CodeIgniter Model": {
		"prefix": "thism",
		"body": "\\$this->load->model('$1');$0"
	},

